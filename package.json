{
  "name": "vue-webpack-boilerplate",
  "version": "1.0.0",
  "description": "A boilerplate by Vue3 and Webpack",
  "main": "main.ts",
  "scripts": {
    "dev": "webpack serve --config ./webpack-config/dev.js",
    "build": "webpack --config ./webpack-config/prod.js",
    "prepare": "husky install",
    "lint": "lint-staged"
  },
  "lint-staged": {
    "src/**/*.js": [
      "eslint --fix",
      "prettier --write"
    ],
    "src/**/*.{css,less,sass,scss,postcss}": [
      "prettier --write"
    ]
  },
  "keywords": [
    "Vue3",
    "Webpack",
    "TypeScript",
    "Boilerplate"
  ],
  "author": "Eathyn",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.23.2",
    "@babel/plugin-transform-runtime": "^7.23.2",
    "@babel/preset-env": "^7.23.2",
    "@babel/runtime-corejs3": "^7.23.2",
    "@types/node": "^20.8.5",
    "babel-loader": "^9.1.3",
    "browserslist": "^4.22.1",
    "css-loader": "^6.8.1",
    "css-minimizer-webpack-plugin": "^5.0.1",
    "dotenv": "^16.3.1",
    "eslint": "^8.51.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-webpack-plugin": "^4.0.1",
    "html-webpack-plugin": "^5.5.3",
    "husky": "^8.0.3",
    "lint-staged": "^15.0.1",
    "mini-css-extract-plugin": "^2.7.6",
    "postcss": "^8.4.31",
    "postcss-loader": "^7.3.3",
    "postcss-preset-env": "^9.2.0",
    "prettier": "3.0.3",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1",
    "webpack-merge": "^5.9.0"
  }
}
